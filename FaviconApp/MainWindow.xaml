<Window x:Class="FaviconApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FaviconApp"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <StackPanel x:Name="ContentPanel" Margin="12,15">
        <Button x:Name="btnGetFavicons" Width="200" Click="HandleGetFavicons">Get Favicons</Button>
        <WrapPanel x:Name="faviconsPanel" Margin="0, 10, 0,0">

        </WrapPanel>
    </StackPanel>
</Window>
